  pod 'KontaktSDK', :git => 'https://github.com/kontaktio/kontakt-ios-sdk.git'
  pod 'Alamofire', '~> 4.3'
  
end

target 'Demo' do
      
    pod 'KontaktSDK', :git => 'https://github.com/kontaktio/kontakt-ios-sdk.git'
    pod 'Alamofire', '~> 4.3'
      
end
